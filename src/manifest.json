{
  "manifest_version": 2,
  "name": "Shorter Google URLs",
  "description": "Share shorter URLs for Google Search.",
  "version": "1.3.1",
  "icons": {
    "16": "link-16.png",
    "48": "link-48.png",
    "128": "link-128.png"
  },
  "background": {
    "scripts": [
      "shorter.js"
    ]
  },
  "page_action": {
    "default_icon": "link-16.png",
    "default_title": "Shorten URL"
  },
  "permissions": [
    "tabs",
    "*://*.google.com/*"
  ]
}
